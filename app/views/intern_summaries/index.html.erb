<p id="notice"><%= notice %></p>

<h1>Listing Intern Summaries</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Content</th>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @intern_summaries.each do |intern_summary| %>
      <tr>
        <td><%= intern_summary.title %></td>
        <td><%= intern_summary.content %></td>
        <td><%= intern_summary.content %></td>
        <td><%= intern_summary.user %></td>
        <td><%= link_to 'Show', intern_summary %></td>
        <td><%= link_to 'Edit', edit_intern_summary_path(intern_summary) %></td>
        <td><%= link_to 'Destroy', intern_summary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intern summary', new_intern_summary_path %>
