<p id="notice"><%= notice %></p>

<h1>Listing Photo Challenges</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Theme</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photo_challenges.each do |photo_challenge| %>
      <tr>
        <td><%= photo_challenge.title %></td>
        <td><%= photo_challenge.theme %></td>
        <td><%= photo_challenge.year %></td>
        <td><%= link_to 'Show', photo_challenge %></td>
        <td><%= link_to 'Edit', edit_photo_challenge_path(photo_challenge) %></td>
        <td><%= link_to 'Destroy', photo_challenge, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Photo challenge', new_photo_challenge_path %>
